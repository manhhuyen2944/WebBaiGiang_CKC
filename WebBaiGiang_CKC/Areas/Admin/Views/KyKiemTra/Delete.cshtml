@model WebBaiGiang_CKC.Models.KyKiemTra

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<ol class="breadcrumb">

    <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Trang chủ</a></li>
    <li class="breadcrumb-item"><a asp-controller="KyKiemTra" asp-action="Index">Quản lý bài kiểm tra</a></li>
    <li class="breadcrumb-item"><a asp-controller="KyKiemTra" asp-action="Delete">Xóa</a></li>
</ol>
<style>
    h1, h3 {
        text-align: center;
    }
</style>
<h1>XÓA KỲ KIỂM TRA</h1>
<h3>Bạn có muốn xóa không?</h3>
<div>
    <br />
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.TenKyKiemTra)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TenKyKiemTra)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ThoiGianBatDau)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ThoiGianBatDau)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ThoiGianKetThuc)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ThoiGianKetThuc)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ThoiGianLamBai)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ThoiGianLamBai)
        </dd>
    </dl>

    <form asp-action="Delete">
        <div style="float: left; padding-right:10px">
            <input type="hidden" asp-for="KyKiemTraId" />
            <input type="submit" value="Xóa" class="btn btn-danger" />
        </div>
        <a class="btn btn-secondary" asp-action="Index">Quay Lại</a>
    </form>
</div>

