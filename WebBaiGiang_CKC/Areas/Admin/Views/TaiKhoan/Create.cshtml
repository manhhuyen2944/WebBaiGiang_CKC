@model WebBaiGiang_CKC.Models.TaiKhoan

@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<ol class="breadcrumb">

    <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Trang chủ</a></li>
    <li class="breadcrumb-item"><a asp-controller="TaiKhoan" asp-action="Index">Quản lý tài khoản</a></li>
    <li class="breadcrumb-item"><a asp-controller="TaiKhoan" asp-action="Create">Thêm</a></li>
</ol>
<h1 style="text-align: center;">THÊM TÀI KHOẢN SINH VIÊN</h1>

<hr />
<br />


<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" method="post" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="MSSV" class="control-label"></label>
                <input asp-for="MSSV" class="form-control" required />
                <span asp-validation-for="MSSV" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group">
                <label asp-for="MatKhau" class="control-label"></label>
                <input asp-for="MatKhau" class="form-control" required />
                <span asp-validation-for="MatKhau" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group">
                <label asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="form-control" required />
                <span asp-validation-for="Email" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group">
                <label asp-for="HoTen" class="control-label"></label>
                <input asp-for="HoTen" class="form-control" required />
                <span asp-validation-for="HoTen" class="text-danger"></span>
            </div>
            <br />

            <div class="form-group">
                <div style="float: left; padding-right:10px">
                    <input type="submit" value="Thêm" class="btn btn-primary" />
                </div>
                <a class="btn btn-secondary" asp-action="Index">Quay Lại</a>
            </div>
        </form>
    </div>
</div>



